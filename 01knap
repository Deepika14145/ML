#include <bits/stdc++.h>
using namespace std;

int main() {
    int n = 4, M = 5;
    int weight[] = {2, 3, 4, 5};
    int profit[] = {3, 4, 5, 6};

    int dp[n + 1][M + 1];

    // Initialize first row and column as 0
    for (int i = 0; i <= n; i++)
        for (int w = 0; w <= M; w++)
            if (i == 0 || w == 0)
                dp[i][w] = 0;

    // Fill the DP table
    for (int i = 1; i <= n; i++) {
        for (int w = 1; w <= M; w++) {
            if (weight[i - 1] <= w)
                dp[i][w] = max(profit[i - 1] + dp[i - 1][w - weight[i - 1]], dp[i - 1][w]);
            else
                dp[i][w] = dp[i - 1][w];
        }
    }

    cout << "Maximum Profit = " << dp[n][M] << endl;

    return 0;
}
